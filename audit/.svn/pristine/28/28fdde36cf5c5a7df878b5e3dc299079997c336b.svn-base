dropdown {
  	position: fixed;
  	left: 0;
  	height: 100%;
  	width: 20%;
  	background-color: #f7f7f7;
  	font-family: "Montserrat", sans-serif;
  	padding-top: 10px;
  	font-size: 12px; 
	border-right: 1px solid;
	margin-top: 80px;
 }
.dropdown li { padding: 0 0 0 20px; margin: 0;padding: 0;list-style: none;}
.dropdown li.selected { background-color: #f2f2f2; }
.dropdown li span {
    display: block;
    width: 100%;
    padding: 10px;
    text-decoration: none;
    cursor: pointer;
}


