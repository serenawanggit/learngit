}


.right_tab table{
	/* overflow : hidden; */
	table-layout:fixed;
	min-width: 100%;
	margin-top: auto;
	margin-bottom: 2%;
}

.right_tab thead th{
	word-break: keep-all;
    white-space: nowrap;
    height: 30px;
    border: 1px solid #19C6A1;
    min-width: 60px;
    background-color: lightseagreen;
    font-weight: bold;
    text-align: center;
    color : white;
    vertical-align: middle;
    font-size : 12px
}

.table_title{
	word-break: keep-all;
    white-space: nowrap;
    height: 20px;
    border: 1px solid #19C6A1;
    min-width: 60px;
    background-color: lightseagreen;
    font-weight: bold;
    vertical-align: middle;
    text-align: center;
    color : white
}

.right_tab tbody td{
	word-break:keep-all; /* for ie */  
	white-space:nowrap; /* for chrome */  
	height:30px;
	border: 1px solid #19C6A1;
	vertical-align: middle;
	text-align: center;
	background-color: white;
	font-size : 12px
}

.right_tab tbody td input[type="text"]{
	width:95%;
	height:95%;
	text-align: center;
	border: 0.1em solid white;
    border-radius: 0.5em;
    -webkit-transition: box-shadow 0.30s ease-in-out;
    -moz-transition:  box-shadow 0.30s ease-in-out; 
    margin-top: 2px;
}

.right_tab tbody td input[type="text"]:focus{
	outline: none;
	border: 0.1em solid #19C6A1;
    box-shadow: 0 0 1px rgba(103, 166, 217, 1); 
}

 .checkbox{
	vertical-align:middle; 
	margin:auto;
	height:30px; 
	border: 0.1em solid #19C6A1;
}

/*  */
.chk_1 { 
    display: none; 
} 
 
.chk_1 + label { 
    background-color: #FFF; 
    border: 1px solid #C1CACA; 
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05); 
    padding: 8px; 
    border-radius: 5px; 
    display: inline-block; 
    position: relative; 
    vertical-align: middle;
} 
.chk_1 + label:active { 
    box-shadow: 0 1px 2px rgba(0,0,0,0.05), inset 0px 1px 3px rgba(0,0,0,0.1); 
} 
 
.chk_1:checked + label { 
    background-color: #ECF2F7; 
    border: 1px solid #92A1AC; 
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05), inset 15px 10px -12px rgba(255, 255, 255, 0.1); 
    color: #243441; 
} 
 
.chk_1:checked + label:after { 
    content: '\2714'; 
    position: absolute; 
    top: 1px; 
    left: 0px; 
    color: #758794; 
    width: 100%; 
    text-align: center; 
    font-size: 0.9em; 
   /*  padding: 1px 0 0 0;  */
    vertical-align: middle; 
} 

