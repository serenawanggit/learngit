package audit.model.quality;

import java.io.Serializable;
import java.util.Date;

/**
 * 质量任务信息
 * @author yjq
 * 2015-4-17
 */
public class AsZlZlrwxx implements Serializable{
    /**
	 * 
	 * AsZlZlrwxx.java
	 */
	private static final long serialVersionUID = -6992347016231848769L;

	private Integer id;
	
	/**
	 * 任务名称
	 */
    private String zlrwmc;
    
    /**
     * 发布时间
     */
    private Date fbsj;

    /**
     * 终止时间
     */
    private Date jzsj;

    /**
     * 任务状态(1:已发布;2:进行中;3：已完成;4：已删除)
     */
    private Integer rwzt;

    /**
     * 任务发布人
     */
    private Integer fbrdm;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public String getZlrwmc() {
        return zlrwmc;
    }

    public void setZlrwmc(String zlrwmc) {
        this.zlrwmc = zlrwmc == null ? null : zlrwmc.trim();
    }

    public Date getFbsj() {
        return fbsj;
    }

    public void setFbsj(Date fbsj) {
        this.fbsj = fbsj;
    }

    public Date getJzsj() {
        return jzsj;
    }

    public void setJzsj(Date jzsj) {
        this.jzsj = jzsj;
    }

    public Integer getRwzt() {
        return rwzt;
    }

    public void setRwzt(Integer rwzt) {
        this.rwzt = rwzt;
    }

    public Integer getFbrdm() {
        return fbrdm;
    }

    public void setFbrdm(Integer fbrdm) {
        this.fbrdm = fbrdm;
    }
}