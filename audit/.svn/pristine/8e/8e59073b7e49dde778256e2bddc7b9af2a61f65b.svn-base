	width: 100%;
	height: 80%;
}


/* 专业评估logo */
.logoclass{
	margin-left:10px;
	margin-top:13px;
	float:left;
	cursor: pointer;
}
.logoclass img{float:left;}
.logoclass label{
	cursor: pointer;
	float:left;
	margin-left:15px;
	height: 70px;
	line-height: 70px;
	font-family: "微软雅黑";
	color: #FFF;
	font-size:18px;
}