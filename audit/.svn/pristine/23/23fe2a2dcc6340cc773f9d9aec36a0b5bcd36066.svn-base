*{ margin:0px; padding:0px;}
body{background: transparent url("../../images/centerbk.png") repeat scroll 0% 0%;}
li{ list-style: none;}
.box{width: 100%; }

.asds_left{width: 19%; float: left; border: 1px solid transparent;}
.asds_right{width: 80%; float: right; border: 1px solid transparent;height:700px;}

.asds_right iframe{width: 100%; height: 99%; }

.qr_box{ width:100%; }
.menuPro{width: 150px}

.li_bg{line-height:24px; padding:0 10px;}

.tb_box{ width:95%;  margin:10px; margin-top: 50px;    margin-left: 5%;}
/*.tb_top{    margin-bottom: 32px;}*/

/* loadding */
.loadding{background-color:#ECE8DE; position:absolute; z-index: 99999; width:100%; height:98%;}
.loadding img{ width:658px;  margin-left:19.5%;}

.tb_title{font-weight:bold;  margin-top: 15px;font-size: x-large; line-height: 35px;}
.tb_title h4{line-height:30px;}
.tb_title h6{}
.tb_title span{float: left; padding-left:10px; line-height:30px;font-size: initial;}
.tb_chocie{float: left;line-height: 35px;font-size:13px;}
.tb_select{width:150px;float: right; margin-top: 7px;}

.tb_main{ width:100%;}
.tb_main table{border: 1px solid #ccc;  border-collapse: collapse; width:100%;margin:5px 0;}
.tb_main table td{border: 1px solid #ccc;text-align: center; height:30px; line-height: 30px; padding: 5px;}
.tb_main table thead{background-color: #eee; border: 1px solid #ccc;}
.tb_main table tfoot td{text-align: left; padding-left:10px; color:#A0A0A0; }

.tb_main table thead h4{float: left; line-height: 30px; padding-left:15px;}

.chocieYear ul li{float: right;line-height:34px;margin: 0px 5px;}
.chocieYear ul li select{border: 1px solid #CCC;}

.face_title1{width: 100%;text-align: center;margin-top: 50px;font-size: xx-large;font-weight: bold;}
.face_title2{width: 100%;text-align: center;font-size: -webkit-xxx-large;margin-top: 36px;}
.face_input{width: 100%;text-align: center;margin-top: 20px;font-size: large;float:left;}
.face_input input{	width: 160%;font-size: large;text-align: center;height:30px;border: 0px; border-bottom-color: black;border-bottom-width: 2px; border-bottom-style: double;margin-left: 20px;}
.face_input span{	width: 160%;text-align: center;height:30px;border: 0px; border-bottom-color: black;border-bottom-width: 2px; border-bottom-style: double;margin-left: 20px;}

.face_foot{width: 100%;text-align: center;margin-top: 60px;font-size: x-large;}


.explain_title{width: 100%;text-align: center;margin-top: 60px;font-size: xx-large;font-weight: bold;}
.explain_content{padding-left: 10%; padding-bottom: 50px;margin-top: 50px; line-height: 25px;}
.explain_content span{width: 90%;float:left;}

input{border: 0px;height:30px;width: 100%;text-align: center;font-size: large;}
textarea{width: 100%;height:100%;font-size: large;}

.input{border: 0px;height:30px;width: 100%;text-align: center;}
.textarea{width: 100%;height:100%;font-size: large;}

.table_content textarea{width: 100%;height:700px;font-size: large;}
.table_content span{width: 100%;height:700px;font-size: large;}
.table4_title{font-size: x-large;font-weight: bold;
   margin-top: 30px;
   margin-left: 100px;}
.table_content{
   margin-top: 20px;
   margin-left: 100px;    width: 80%;}
 .table_tr_title{    line-height: 30px; font-weight: bold; }
   
.table5_div{border: 1px solid;height:25%;width: 85%;margin-left: 100px; }
.table5_div textarea{width: 80%;height:70%;font-size: large;border:0px;    margin-top: 5px;}
.table5_div span{width:98%;height:70%;font-size: large;border:0px; margin-top: 5px; }
.table5_div div{    margin-top: 10px;
   float: right;
   margin-right: 20px;}
.table5_title{font-size: x-large;
   margin-top: 20px;
   margin-left: 100px;}

.button_input {
	margin-left: 10px;
	width: 63px;
	height: 25px;
	line-height: 17px;
	text-align: center;
	border: 1px solid #19C6A1;
	border-radius: 4px;
	background-color: #19C6A1;
	color: #EAF9F5;
	font-size: 13px;
	font-weight: bold;
	cursor: pointer;
	margin-top:8px;
	margin-right: 5px;
}
   