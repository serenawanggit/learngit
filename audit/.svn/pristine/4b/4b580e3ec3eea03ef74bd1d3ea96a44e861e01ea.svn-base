function changeTab(type,obj){
	var container=$('#contextIframe');
	$('.navbar-nav li a').css("border-bottom", "0px solid #404A58");
	var obj = $(obj);
	obj.css("border-bottom", "2px solid #19C6A1");
	switch(type){
	case 1:
		container.attr("src","/audit/page/majorEvaluation/conditionEstimationStatus.jsp");
		break;
	case 2:
		container.attr("src","/audit/page/majorEvaluation/conditionEstimationSum.jsp");
		break;
	case 3:
		container.attr("src","/audit/page/majorEvaluation/conditionEstimationAnalysis.jsp");
		break;
	}
	//alert(container.attr("src"));
		
}