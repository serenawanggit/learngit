package audit.model.quality;

import java.io.Serializable;
import java.util.Date;
/**
 * 阶段任务人员关联
 * @author yjq
 * 2015-4-17
 */
public class AsZlJdrwryglb implements Serializable{
    /**
	 * 
	 */
	private static final long serialVersionUID = -232307837400307300L;

	private Integer id;

	/**
	 * 阶段任务id
	 */
    private Integer jdrwid;
    
    /**
     * 填报人ID
     */
    private Integer tbrid;

    /**
     * 审核人id
     */
    private Integer shrid;

    /**
     * 阶段任务状态 0：未完成；1:已完成
     */
    private Integer status;
    
    /**
     * 完成时间
     */
    private Date wcsj;

    /**
     * 审核时间
     */
    private Date shsj;

    public Date getWcsj() {
		return wcsj;
	}

	public void setWcsj(Date wcsj) {
		this.wcsj = wcsj;
	}

	public Date getShsj() {
		return shsj;
	}

	public void setShsj(Date shsj) {
		this.shsj = shsj;
	}

	public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getJdrwid() {
        return jdrwid;
    }

    public void setJdrwid(Integer jdrwid) {
        this.jdrwid = jdrwid;
    }

    public Integer getTbrid() {
        return tbrid;
    }

    public void setTbrid(Integer tbrid) {
        this.tbrid = tbrid;
    }

    public Integer getShrid() {
        return shrid;
    }

    public void setShrid(Integer shrid) {
        this.shrid = shrid;
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }
}