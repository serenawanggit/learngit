function getImg(img,fileName){
	$.ajax({
		async:false,
		url : "/CHART/GetASDSR_Table_Img",
		type : "POST",
		data : {"img_Base64":img,"img_Name": fileName},
		error:function(){
			if(isFirst){
				alert("save image error !");
				isFirst=false;
			}
		}
	});
}
